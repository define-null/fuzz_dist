# build antidote from host shared volume to container shared volume
#    'rm -rf /var/jepsen/shared/antidote; \
docker exec \
    -it \
    -w /jepsen \
    jepsen-control \
    bash -c \
    'cd /var/jepsen/shared; \
    git clone https://github.com/AntidoteDB/antidote.git;
    cd antidote; \
    make rel'
